pipeline {
    agent any

    stages {
      stage('Build') {
     	when {
             expression{
                 BRANCH_NAME == "master"
             }
        }    
            steps {
		        mvn clean compile test-compile
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
